{
  "compilerOptions": {
    "outDir": "tmp",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": [
      "es6", "es7", "es2015", "es2016", "es2017", "dom"
    ],
    "typeRoots": [
      "./node_modules/@types/",
      "./src/interface/"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strictNullChecks": true
  }
}
